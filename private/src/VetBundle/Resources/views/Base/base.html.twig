<!DOCTYPE html>

<html>

<head>
    <title>Vet - {% block titleBase %}{% endblock %}</title>
    <meta charset="UTF-8">
    {% block head %}{% endblock %}
    <!--   viewport, pas de zoom -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no"/>
    <!--    POLICE -->
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,400italic,600,600italic,700,700italic'
          rel='stylesheet' type='text/css'>
    <!--    BOOTSTRAP -->
    <link href="/bootstrap/css/bootstrap.min.css" media="print,screen" rel="stylesheet"/>
    <!-- DATATABLE CSS -->
    <link rel="stylesheet" href="/css/dataTable.min.css">
    <!--    JQUERY -->
    <script type="text/javascript" src="/scripts/jquery-3.1.1.min.js"></script>
    <!-- BOOTSTRAP TOGGLE CSS -->
    <link href="https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css" rel="stylesheet">
    <!--   ALC CSS -->
    <link href="/Vet/alc.css" media="print,screen" rel="stylesheet"/>
</head>

<body>
<div class="container">
    <br>
    {{ include('VetBundle:Base:nav.html.twig') }}

    {% for flash_message in app.session.flashBag.get('error') %}
        <div class="flash-notice flash-bg-red">
            <p>{{ flash_message }}</p>
        </div>
    {% endfor %}
    {% for flash_message in app.session.flashBag.get('success') %}
        <div class="flash-notice flash-bg-24bd24">
            <p>{{ flash_message }}</p>
        </div>
    {% endfor %}


    {% block principal %}{% endblock %}
    <br>
</div>
<div class="container2">
    {{ include('VetBundle:Base:footer.html.twig') }}
</div>
{% block script %}
    <!-- DATATABLE JS -->
    <script type="text/javascript" src="/scripts/dataTable.min.js"></script>

    <script type="text/javascript" src="/bootstrap/js/bootstrap.min.js"></script>

    <script type="text/javascript" src="/vet/alc.js"></script>
{% endblock %}
</body>

</html>



